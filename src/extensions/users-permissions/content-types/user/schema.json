{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "phone": {
      "type": "string"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "favArtists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::artist.artist"
    },
    "favAlbums": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::album.album"
    },
    "favSongs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::song.song"
    },
    "playlists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::playlist.playlist",
      "mappedBy": "user"
    },
    "premium": {
      "type": "enumeration",
      "enum": [
        "none",
        "freeMonthly",
        "individualMonthly",
        "studentMonthly",
        "individualYearly",
        "studentYearly"
      ],
      "default": "none"
    },
    "lastDatePremium": {
      "type": "date"
    },
    "usedFreePremium": {
      "type": "boolean",
      "default": false
    }
  }
}
